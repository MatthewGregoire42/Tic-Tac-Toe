<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<StackPane xmlns:fx="http://javafx.com/fxml" fx:controller="gui.FinishController">
    <ImageView fx:id="imgView"
               disable="false"
               preserveRatio="true" />
    <VBox spacing="40"
          xmlns:fx="http://javafx.com/fxml"
          fx:id="vbox">
        <Label text="Game over!" opacity="1"/>
        <Label fx:id="label" opacity="1"/>
        <Button text="Replay" onAction="#backToPlay" opacity="1"/>
        <Button text="Back to Menu" onAction="#backToStart" opacity="1"/>

        <prefWidth>600</prefWidth>
        <prefHeight>600</prefHeight>
        <alignment>CENTER</alignment>
    </VBox>
</StackPane>